{
  "name": "tradely-locates-server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "ts": "tsc -p .",
    "start": "npm run build && node build/src/index.js",
    "locates": "npm run build && node build/src/locates.js",
    "telegram": "npm run build && node build/src/bot.js",
    "server": "npm run build && node build/http-server.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "clean": "rimraf ./build",
    "build": "npm run clean && tsc",
    "local": "ts-node --esm src",
    "local:watch": "nodemon src -e ts,json --exec \"yarn local\""
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/got": "^9.6.12",
    "cheerio": "^1.0.0-rc.12",
    "express": "^4.18.2",
    "got": "^13.0.0",
    "node-telegram-bot-api": "^0.61.0",
    "querystring": "^0.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@types/node-telegram-bot-api": "^0.61.6",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.1.3"
  },
  "licenses": [
    "MIT"
  ],
  "description": ""
}
